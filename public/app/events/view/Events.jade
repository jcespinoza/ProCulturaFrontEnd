h1(style="text-align:center;") Events

div(style ="width:100%;height:100px;border:2px solid ;" ng-repeat="event in events")
    img(ng-src="{{event.ImageUrl}}" style="width:80px; height:90px;float:left; margin:3px")
    h1 {{event.Name}}
    p Start Date: {{event.startDate}}
    a(ng-href="http://localhost:8090/event/{{event.EventId}}" style="float:right;") Details
    p Location: {{event.Location}}
    br
    br
